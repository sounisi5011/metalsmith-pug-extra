{
  "name": "metalsmith-pug-extra",
  "version": "1.0.1",
  "description": "Metalsmith plugin to convert or compile and render Pug files.",
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "plugin",
    "pug",
    "template",
    "transform"
  ],
  "homepage": "https://github.com/sounisi5011/metalsmith-pug-extra#readme",
  "bugs": {
    "url": "https://github.com/sounisi5011/metalsmith-pug-extra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sounisi5011/metalsmith-pug-extra.git"
  },
  "license": "MIT",
  "author": "sounisi5011",
  "files": [
    "dist/"
  ],
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts ./",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc -p ./test/ --noEmit",
    "test": "run-s lint test:ava",
    "test:ava": "ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(package).json|*.{yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@types/debug": "4.1.4",
    "@types/deep-freeze-strict": "1.1.0",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/metalsmith": "2.3.0",
    "@types/pug": "2.0.4",
    "clone-deep": "4.0.1",
    "debug": "4.1.1",
    "deep-freeze-strict": "1.1.1",
    "is-utf8": "0.2.1",
    "lodash.clonedeep": "4.5.0",
    "multimatch": "4.0.0",
    "pug": "2.0.4"
  },
  "devDependencies": {
    "@types/sinon": "7.0.13",
    "@types/slug": "0.9.1",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "ava": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "5.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.5.0",
    "lint-staged": "8.2.1",
    "metalsmith": "2.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "prettier-package-json": "2.1.0",
    "sinon": "7.3.2",
    "slug": "1.1.0",
    "sort-package-json": "1.22.1",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  },
  "peerDependencies": {
    "metalsmith": ">=2.3.0"
  },
  "engines": {
    "node": ">=8.15.0"
  },
  "ava": {
    "files": [
      "./test/**/*.ts"
    ],
    "helpers": [
      "**/helpers/**/*"
    ],
    "snapshotDir": "./test/.snapshots/ava/",
    "compileEnhancements": false,
    "require": [
      "ts-node/register/transpile-only"
    ],
    "extensions": [
      "ts"
    ]
  }
}
