{
  "name": "metalsmith-pug-extra",
  "version": "0.0.0",
  "private": true,
  "description": "Metalsmith plugin to convert or compile and render Pug files.",
  "repository": "git@github.com:sounisi5011/metalsmith-pug-extra.git",
  "license": "MIT",
  "author": "sounisi5011",
  "main": "dist/index",
  "scripts": {
    "lint": "eslint --ext .ts ./",
    "test": "run-s lint test:ava",
    "test:ava": "ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(package).json|*.{yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "multimatch": "^4.0.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/metalsmith": "^2.3.0",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "ava": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "metalsmith": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-package-json": "^2.1.0",
    "sort-package-json": "^1.22.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "ava": {
    "files": [
      "./test/"
    ],
    "snapshotDir": "./test/.snapshots/ava/",
    "compileEnhancements": false,
    "require": [
      "ts-node/register/transpile-only"
    ],
    "extensions": [
      "ts"
    ]
  }
}
